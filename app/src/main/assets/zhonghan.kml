<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>中韩渔业协定暂定措施水域</name>
    <!-- 自定义样式：蓝色半透明填充+红色边界线 -->
    <Style id="provisionalAreaStyle">
      <PolyStyle>
        <color>7fff40a0</color> <!-- 半透明蓝色，格式为透明度+颜色十六进制 -->
        <outline>1</outline> <!-- 显示边界线 -->
